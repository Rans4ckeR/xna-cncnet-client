<Project>
  <PropertyGroup>
    <TargetFrameworks Condition="'$(Engine)' != 'UniversalGL'">net8.0-windows;net7.0-windows</TargetFrameworks>
    <TargetFrameworks Condition="'$(Engine)' == 'UniversalGL'">net8.0;net7.0</TargetFrameworks>
    <UseWindowsForms Condition="'$(Engine)' != 'UniversalGL'">true</UseWindowsForms>
    <ComVisible>false</ComVisible>
    <CLSCompliant>false</CLSCompliant>
    <AnalysisLevel>preview-all</AnalysisLevel>
    <LangVersion>preview</LangVersion>
    <EnforceCodeStyleInBuild>true</EnforceCodeStyleInBuild>
    <Nullable>disable</Nullable>
    <ImplicitUsings>disable</ImplicitUsings>
  </PropertyGroup>
</Project>